<div class="row">
  <div class="col-md-4">
    <div class="well well-sm">
      Municipio
      <hr>
      <select class="form-control search2" id="select_municipality" 
        name="municipality_id">
        <option value="-1"> </option>
        <option value="0"> Otros </option>
        <% @municipalities.each do |mun| %>
          <option value="<%= mun.id %>"> <%= mun.name %></option>
        <% end %>
      </select>      
    </div>
  </div>
  <div class="col-md-4">
    <div class="well well-sm">
      Fecha de Registro
      <hr>
      <div class="row">
        <div class="col-md-6">
          <input type="text" placeholder="Fecha Inicio" 
          class="form-control search2 datepicker" name="register_start_date" />
        </div>
        <div class="col-md-6">
          <input type="text" placeholder="Fecha Fin" 
          class="form-control search2 datepicker" name="register_end_date"/>
        </div>
      </div>
    </div>
  </div>
  <div class="col-md-4">
    <div class="well well-sm">
      Fecha de Nacimiento
      <hr>
      <div class="row">
        <div class="col-md-6">
          <input type="text" placeholder="Fecha Inicio" 
          class="form-control search2 datepicker" name="bird_start_date"/>
        </div>
        <div class="col-md-6">
          <input type="text" placeholder="Fecha Fin" 
          class="form-control search2 datepicker" name="bird_end_date"/>
        </div>
      </div>
    </div>
  </div>
</div>
